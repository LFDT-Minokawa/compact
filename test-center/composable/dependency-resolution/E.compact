import CompactStandardLibrary;

contract B {
    circuit up(x: Uint<16>): [];
    circuit down(x: Uint<16>): [];
}

contract D {
    circuit up(x: Uint<16>): [];
    circuit down(x: Uint<16>): [];
}

export ledger b: B;
export ledger d: D;

constructor(bParam: B, dParam: D) {
    b = disclose(bParam);
    d = disclose(dParam);
}

export circuit up(i: Uint<16>): [] {
    b.up(disclose(i));
    d.up(disclose(i));
}

export circuit down(i: Uint<16>): [] {
    b.down(disclose(i));
    d.up(disclose(i));
}